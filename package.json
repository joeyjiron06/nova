{
  "name": "nova",
  "version": "1.0.0-beta.1",
  "description": "A universal caching library that can be used in any javascript runtime (e.g. nodejs, browser, bun, deno, etc.)",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "lint": "eslint"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./store/memory": {
      "types": "./dist/src/stores/memoryStore.d.ts",
      "default": "./dist/src/stores/memoryStore.js"
    },
    "./store/filesystem": {
      "types": "./dist/src/stores/fsStore.d.ts",
      "default": "./dist/src/stores/fsStore.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "cache",
    "universal",
    "cache",
    "nodejs",
    "cache",
    "node",
    "cache",
    "ttl",
    "cache",
    "lru",
    "cache",
    "browser",
    "cache"
  ],
  "author": "Joey Jiron <joeyjiron06@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.0",
    "@vitest/coverage-v8": "4.0.15",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "fs-extra": "^11.3.2",
    "superjson": "^2.2.6"
  },
  "packageManager": "pnpm@10.11.0"
}